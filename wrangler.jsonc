{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "frasal",
	"main": "src/index.ts",
	"compatibility_date": "2025-07-15",
	"assets": {
		"directory": "./public/",
		"binding": "ASSETS",
		"run_worker_first": [
			"/"
		]
	},
	"observability": {
		"enabled": true,
		"logs": {
		  "invocation_logs": true,
		  "head_sampling_rate": 1
		}
	}
}